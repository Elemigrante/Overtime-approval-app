<% pending_audit_confirmations.each do |pac| %>
  <div class="col-md-4 mb-3">
    <%= link_to "I Confirm that I did not perform any overtime for the week of: #{pac.start_date}", confirm_audit_log_path(pac),
                id:    "confirm_#{pac.id}",
                class: 'btn btn-primary btn-block btn-lg',
                data:  { confirm: 'Are you sure you want to confirm that you did not perform any overtime?' } %>
  </div>
<% end %>